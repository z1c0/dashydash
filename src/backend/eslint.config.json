{
	"root": true,
	"extends": ["eslint:recommended"],
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint"],
	"env": {
		"node": true,
		"es6": true
	},
	"rules": {
		"@typescript-eslint/no-explicit-any": "error",
		"no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
		"no-console": "off"
	},
	"globals": {
		"fetch": true
	}
}
